# Supabase Configuration
# Set your Supabase database password here
SUPABASE_DB_PASSWORD="your-supabase-database-password"

# Database Connection String
# The password will be automatically substituted from SUPABASE_DB_PASSWORD
DATABASE_URL="postgresql://postgres:${SUPABASE_DB_PASSWORD}@db.your-project-ref.supabase.co:5432/postgres"

# Admin
ADMIN_PASSWORD="your-secure-password-here"

# Security
SESSION_SECRET="your-random-session-secret-at-least-32-chars"
ALLOWED_ORIGINS="http://localhost:3000,https://your-app.amplifyapp.com"
